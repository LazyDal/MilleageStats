{
  "name": "Milleage",
  "version": "0.0.0",
  "description": "Web app for your cars",
  "main": "app/app.js",
  "scripts": {
    "buildjsx:DetailsView": "jsx app/views-jsx/DetailsView.jsx > app/views/DetailsView.js",
    "buildjsx:CarsBox": "npm run buildjsx:CarCard && jsx app/views-jsx/CarsBox.jsx > app/views/CarsBox.js",
    "buildjsx:CarCard": "npm run buildjsx:CarStats && jsx app/views-jsx/CarCard.jsx > app/views/CarCard.js",
    "buildjsx:app": "jsx app/app.jsx > app/app.js",
    "build:webpack": "webpack app/app.js static/js/bundle.js",
    "build": "npm run buildjsx:CarsBox && npm run buildjsx:DetailsView && npm run buildjsx:app && npm run build:webpack",
    "test": "jest",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LazyDal/MilleageStats"
  },
  "author": "Dalibor Dragojevic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LazyDal/MilleageStats/issues"
  },
  "dependencies": {
    "express": "~4.11.2",
    "body-parser": "~1.12.0",
    "mongoose": "~3.8.23",
    "backbone": "~1.1.2",
    "jquery": "~2.1.3",
    "react": "~0.12.2",
    "underscore": "~1.8.2"
  }
}
